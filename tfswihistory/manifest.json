{
  "name" : "Azure DevOps workitem info",
  "author" : "atotmakov",
  "version" : "0.0.0.6",
  "description" : "Get info for Azure DevOps workitems, including history data",
  "background" : { "scripts": ["background.js"], "persistent": false },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["dt.js"]
    }
  ],
  "permissions" : [
    "tabs",	
    "storage",
    "contextMenus",
    "http://*/*",
    "https://*/*"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "manifest_version": 2
}
